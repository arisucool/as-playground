{
  "name": "as-playground",
  "version": "0.0.0",
  "scripts": {
    "dev": "ng serve --host=0.0.0.0 --disable-host-check --ssl",
    "deploy": "ng deploy --base-href=/as-playground/ --repo=https://github.com/arisucool/as-playground.git",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "commit": "cz",
    "prepare": "husky install"
  },
  "private": true,
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "subject-case": [
        0
      ]
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog",
      "disableSubjectLowerCase": true
    }
  },
  "author": "arisu.cool üçì",
  "license": "MIT",
  "dependencies": {
    "@angular/animations": "~12.2.15",
    "@angular/cdk": "^12.2.13",
    "@angular/common": "~12.2.15",
    "@angular/compiler": "~12.2.15",
    "@angular/core": "~12.2.15",
    "@angular/forms": "~12.2.15",
    "@angular/material": "^12.2.13",
    "@angular/platform-browser": "~12.2.15",
    "@angular/platform-browser-dynamic": "~12.2.15",
    "@angular/router": "~12.2.15",
    "@swimlane/ngx-charts": "^20.1.0",
    "@uriopass/nosleep.js": "^0.12.1",
    "angularx-qrcode": "^12.0.0",
    "crypto-js": "^4.1.1",
    "file-saver": "^2.0.5",
    "jsstore": "^4.3.7",
    "nicojs": "github:mugiply/nicoJS",
    "qrcode-parser": "^2.0.4",
    "rxjs": "~6.6.0",
    "skyway-js": "^4.4.3",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.15",
    "@angular/cli": "~12.2.15",
    "@angular/compiler-cli": "~12.2.15",
    "@commitlint/cli": "^16.0.1",
    "@commitlint/config-conventional": "^16.0.0",
    "@types/crypto-js": "^4.1.1",
    "@types/file-saver": "^2.0.5",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "angular-cli-ghpages": "^1.0.0",
    "codelyzer": "^6.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^7.0.4",
    "jasmine-core": "~3.8.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.12",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.3.5"
  }
}
